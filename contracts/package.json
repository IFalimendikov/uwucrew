{
  "name": "uwucrew",
  "version": "1.0.0",
  "description": "POG",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "yarn compile && yarn test:coverage",
    "test:coverage": "hardhat coverage",
    "gas": "yarn compile && hardhat test --network hardhat",
    "deploy:local": "hardhat test && hardhat run --network localhost scripts/deploy.js",
    "deploy:test": "hardhat test && hardhat run --network rinkeby scripts/deploy.js",
    "deploy:arbitrum": "hardhat test && hardhat run --network arbitrum scripts/deploy.js",
    "deploy:ethereum": "hardhat test && hardhat run --network ethereum scripts/deploy.js",
    "postcompile": "yarn abi:copy:frontend && yarn abi:copy:api",
    "abi:copy:frontend": "copyfiles -u 1 ./abis/* ../client/src/contracts",
    "abi:copy:api": "copyfiles -u 1 ./abis/* ../api/functions/contracts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waifusion/v2"
  },
